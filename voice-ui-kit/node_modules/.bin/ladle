#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/lib/cli/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/lib/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/lib/cli/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/lib/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/react/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules/@ladle/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/@ladle+react@5.0.3_@types+node@22.18.8_@types+react@19.1.16_jiti@2.6.0_lightningcss@1.3_8f285fcfc86602d452a6977bd7d0c557/node_modules:/home/jireh/Documents/projects/voice-ui-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ladle/react/lib/cli/cli.js" "$@"
else
  exec node  "$basedir/../@ladle/react/lib/cli/cli.js" "$@"
fi
